{
  "name": "nexspace",
  "private": true,
  "workspaces": [
    "nexspace-frontend",
    "nexspace-backend"
  ],
  "scripts": {
    "dev": "concurrently -k \"npm:dev-*\"",
    "dev-frontend": "npm --workspace nexspace-frontend run dev",
    "dev-backend": "npm --workspace nexspace-backend run dev",
    "build": "npm --workspace nexspace-frontend run build && npm --workspace nexspace-backend run build"
  },
  "devDependencies": {
    "concurrently": "^9.0.0"
  },
  "dependencies": {
    "react-router-dom": "^7.8.2"
  }
}
