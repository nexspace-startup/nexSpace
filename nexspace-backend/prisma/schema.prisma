generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         BigInt @id @default(autoincrement())
  first_name String @db.VarChar(100)
  last_name  String @db.VarChar(100)
  role       Int
}
